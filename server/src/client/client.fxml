<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.ClientController" prefHeight="200.0" prefWidth="300.0">
    <TextField fx:id="ipTextField" layoutX="50.0" layoutY="50.0" promptText="Enter server IP" />
    <Button layoutX="100.0" layoutY="100.0" text="Connect" onAction="#handleConnectButton" />
    <Button fx:id="disconnectButton" text="Disconnect" layoutX="50.0" layoutY="150.0" onAction="#handleDisconnect" />
</AnchorPane>
